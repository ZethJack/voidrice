vo=xv
xv-adaptor=1
x11-bypass-compositor=no
profile=opengl-hq
scale=ewa_lanczossharp
cscale=ewa_lanczossoft
dscale=mitchell
scale-antiring=0.7
cscale-antiring=0.8
dither-depth=auto
correct-downscaling=yes
sigmoid-upscaling=yes
deband=no
volume-max=100
hwdec=auto
ytdl-format='bestvideo[height<=480][fps=30][vcodec!=vp9]+bestaudio/best'
ytdl-raw-options='prefer-free-formats='
ytdl-raw-options='buffer-size=2048'
